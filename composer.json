    "homepage": "http://www.foolz.us",
    "license": "Apache-2.0",
    "authors": [{"name": "foolz", "email": "support@foolz.us"}],
    "support": {
        "email": "support@foolz.us",
        "irc": "irc://irc.irchighway.net/fooldriver"
    },
    "require": {
        "php": ">=5.4.0",
		"doctrine/dbal": ">=2.3.0",
		"ircmaxell/password-compat": "dev-master",
		"foolz/inet": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Foolz\\Auth": "classes/"
        }
    }
}